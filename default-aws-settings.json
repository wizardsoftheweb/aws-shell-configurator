{
  "profile": "default",
  "settings": {
    "aws_access_key_id": {
      "env": "AWS_ACCESS_KEY_ID"
    },
    "aws_secret_access_key": {
      "env": "AWS_SECRET_ACCESS_KEY"
    },
    "aws_session_token": {
      "env": "AWS_SESSION_TOKEN"
    },
    "output": {
      "env": "AWS_DEFAULT_OUTPUT",
      "value": "json",
      "allowed": ["json", "table", "text"]
    }
  }
}
